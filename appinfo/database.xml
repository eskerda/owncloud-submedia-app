<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>

 <name>*dbname*</name>
 <create>true</create>
 <overwrite>false</overwrite>

 <charset>utf8</charset>

 <table>

  <name>*dbprefix*submedia_playlists</name>

  <declaration>

   <field>
    <name>id</name>
    <type>integer</type>
    <default>0</default>
    <notnull>true</notnull>
    <autoincrement>1</autoincrement>
    <unsigned>true</unsigned>
    <length>4</length>
   </field>

   <field>
    <name>userid</name>
    <type>text</type>
    <default></default>
    <notnull>true</notnull>
    <length>255</length>
   </field>

   <field>
    <name>name</name>
    <type>text</type>
    <default></default>
    <notnull>true</notnull>
    <length>255</length>
   </field>

   <field>
    <name>created</name>
    <type>timestamp</type>
    <default></default>
    <notnull>false</notnull>
    <length></length>
   </field>
  </declaration>

 </table>

 <table>

  <name>*dbprefix*submedia_playlist_song</name>

  <declaration>

   <field>
    <name>playlist_id</name>
    <type>integer</type>
    <default></default>
    <notnull>true</notnull>
    <unsigned>true</unsigned>
    <length>4</length>
   </field>

   <field>
    <name>song_id</name>
    <type>integer</type>
    <default></default>
    <notnull>false</notnull>
    <length>4</length>
   </field>

  </declaration>
 
 </table>

</database>
